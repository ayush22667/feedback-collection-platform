{
  "name": "feedback-collection-platform",
  "version": "1.0.0",
  "description": "A full-stack feedback collection platform built with MERN stack",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "setup": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "feedback",
    "forms",
    "mern",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Ayush Anand",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
